{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outFile": "build/codewords_core.js",
    "module": "System",
    "target": "ES5",
    "lib": [
      "DOM",
      "ES5",
      "ES2015.Iterable"
    ],
    "downlevelIteration": true,
    "strictNullChecks": true
  },
  "include": [
    "src/*.ts",
    "src/**/*.ts"
  ]
}
